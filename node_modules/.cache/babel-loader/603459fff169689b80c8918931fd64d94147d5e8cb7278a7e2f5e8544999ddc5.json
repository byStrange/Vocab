{"ast":null,"code":"import Nav from \"./components/Nav.vue\";\nimport Box from \"./components/Box.vue\";\nimport List from \"./components/List.vue\";\nimport Panel from \"./components/Panel.vue\";\nexport default {\n  data() {\n    return {\n      data_switched: \"off\",\n      showWord: false,\n      choosenTopic: null,\n      word: null,\n      fullWord: null\n    };\n  },\n  methods: {\n    changeShowWord(value) {\n      this.showWord = value;\n    },\n    startTest($event) {\n      this.choosenTopic = $event;\n      this.fullWord = this.choosenTopic.words.shift();\n      this.word = this.fullWord[Math.floor(Math.random() * 2)];\n    },\n    nextWord() {\n      if (!this.choosenTopic.words.length) {\n        this.word = \"THE END\";\n        return;\n      }\n      this.word = this.choosenTopic.words.shift()[Math.floor(Math.random() * 2)];\n    },\n    makeWordReversed() {\n      this.word = this.fullWord[0] === this.word ? this.fullWord[1] : this.fullWord[0];\n    }\n  },\n  name: \"App\",\n  components: {\n    Nav,\n    Box,\n    List,\n    Panel\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,GAAE,MAAO,sBAAsB;AACtC,OAAOC,GAAE,MAAO,sBAAsB;AACtC,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,KAAI,MAAO,wBAAwB;AAE1C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,IAAG;MACjBC,IAAI,EAAE,IAAI;MAACC,QAAQ,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAc,CAACC,KAAK,EAAE;MACpB,IAAI,CAACN,QAAO,GAAIM,KAAK;IACvB,CAAC;IACDC,SAAS,CAACC,MAAM,EAAE;MAChB,IAAI,CAACP,YAAW,GAAIO,MAAM;MAC1B,IAAI,CAACL,QAAO,GAAI,IAAI,CAACF,YAAY,CAACQ,KAAK,CAACC,KAAK,EAAC;MAC9C,IAAI,CAACR,IAAG,GAAK,IAAI,CAACC,QAAQ,CAACQ,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAI,CAAC,CAAC,CAAC;IAC3D,CAAC;IACDC,QAAQ,GAAE;MACR,IAAI,CAAC,IAAI,CAACb,YAAY,CAACQ,KAAK,CAACM,MAAM,EAAE;QACnC,IAAI,CAACb,IAAG,GAAI,SAAS;QACrB;MACF;MACA,IAAI,CAACA,IAAG,GAAG,IAAI,CAACD,YAAY,CAACQ,KAAK,CAACC,KAAK,EAAE,CAACC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAI,CAAC,CAAC;IAC1E,CAAC;IACDG,gBAAgB,GA